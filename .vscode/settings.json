{
    "breadcrumbs.enabled": true,
    "ccls.launch.args": [
        "-std=C11"
    ],
    "C_Cpp.clang_format_fallbackStyle": "LLVM",
    "C_Cpp.clang_format_sortIncludes": true,
    "C_Cpp.clang_format_style": "LLVM",
    "C_Cpp.autocomplete": "Disabled",
    "C_Cpp.formatting": "Disabled",
    "C_Cpp.errorSquiggles": "Disabled",
    "C_Cpp.intelliSenseEngine": "Disabled",
    // "codeui.targetingMode": "themeSpecific",
    // "clang.cflags": [
    //     "-std=c11",
    // ],
    // "clang.cxxflags": [
    //     "-std=c++11"
    // ],
    "cmake.cmakePath": "/usr/bin/cmake",
    "cpplint.cpplintPath": "/usr/bin/cpplint",
    // "c-cpp-flylint.clang.executable": "clang",
    "editor.fontSize": 14,
    "editor.largeFileOptimizations": false,
    "editor.minimap.enabled": true,
    "editor.minimap.renderCharacters": true,
    "editor.scrollBeyondLastLine": false,
    "editor.snippetSuggestions": "top",
    "editor.suggestSelection": "first",
    "editor.wordWrap": "on",
    "explorer.confirmDelete": false,
    "files.associations": {
        "justfile": "makefile",
        "*.lex": "lex",
        "*.gw": "chuck",
        "*.tidal": "haskell",
        "conky-i3statusbar": "lua",
        "system-overview": "lua"
    },
    "files.enableTrash": true,
    "files.autoSave": "afterDelay",
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "licenser.author": "Andrew Prentice",
    "licenser.license": "MIT",
    "licenser.projectName": "tgtracker",
    "licenser.useSingleLineStyle": true,
    "ccls.codeLens.renderInline": true,
    "ccls.codeLens.localVariables": true,
    "ccls.completion.detailedLabel": true,
    "ccls.diagnostics.spellChecking": false,
    "ccls.index.threads": 8,
    "ccls.misc.compilationDatabaseCommand": "compiledb",
    "cmake.configureOnOpen": true,
    "cmake.generator": "Ninja",
    "cmake.installPrefix": "build",
    "cmake.cpptools.intelliSenseMode": "gcc-x64",
    "cmake.reconfigureOnChange": true,
}